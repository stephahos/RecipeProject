<div class="col-autodetails">
    <a href="/recipes/myprofilepage" class="btn btn-warning"> Go Back </a>
    </div>

<div class="myrecipes">
    <h3> My Recipes</h3>
</div>

<div class="ul-allrecipes">

<% currentUser.created.forEach(oneRecipe => { %>

<div class="cardAll">

    <a href="/recipes/myrecipes/<%- oneRecipe._id %>"> 
        <img class='card_imageAll' src = '<%- oneRecipe.image %>'/>  
    </a>
<div class="containertext">

<div class="card_timeandlevel">
    <h3><%= oneRecipe.name %></h3> 
    <h4><%= oneRecipe.cooktime %> min</h4>
    <h4><%= oneRecipe.level %></h4>
</div>

</div>
</div>
<% }) %>
</div>