<h2>Recipes by result</h2>

<% results.forEach(recipe => { %>
  <a href="/recipes/results/<%- recipe._id %>"> <img class='card_imageAll' src = '<%- recipe.image %>'/>  
    <div class="card_title"><p><%= recipe.name %></p> </div>
    <div class="card_cooktime"><p><%= recipe.cooktime %></p></div>
    <div class="card_level"><p><%= recipe.level %></p></div>

<% }) %>

